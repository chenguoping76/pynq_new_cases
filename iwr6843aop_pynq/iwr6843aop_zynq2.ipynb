    "    def start_auto_update(self):\n",
    "        \"\"\"启动自动更新\"\"\"\n",
    "        if self.update_flag:\n",
    "            print(\"ℹ️ 可视化已在更新中\")\n",
    "            return\n",
    "        self.update_flag = True\n",
    "        self.auto_thread = Thread(target=self._auto_update_thread, daemon=True)\n",
    "        self.auto_thread.start()\n",
    "        print(f\"✅ 可视化自动更新已启动（间隔：{self.update_interval}ms）\")\n",
    "\n",
    "    def stop_auto_update(self):\n",
    "        \"\"\"停止自动更新\"\"\"\n",
    "        self.update_flag = False\n",
    "        if hasattr(self, \"auto_thread\") and self.auto_thread.is_alive():\n",
    "            self.auto_thread.join(timeout=1)\n",
    "        # 清空图表\n",
    "        if self.fig_container:\n",
    "            with self.fig_2d.batch_update():\n",
    "                self.fig_2d.data[0].r = []\n",
    "                self.fig_2d.data[0].theta = []\n",
    "            with self.fig_3d.batch_update():\n",
    "                self.fig_3d.data[0].x = []\n",
    "                self.fig_3d.data[0].y = []\n",
    "        print(\"✅ 可视化自动更新已停止\")"
