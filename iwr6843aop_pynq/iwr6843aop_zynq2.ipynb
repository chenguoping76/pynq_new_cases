    "            marker=dict(\n",
    "                size=5,\n",
    "                color=[],\n",
    "                colorscale=\"RdYlBu_r\",\n",
    "                # 配置文件最大速度1m/s\n",
    "                cmin=-1, cmax=1,\n",
    "                colorbar_title=\"多普勒速度 (m/s)\"\n",
    "            ),\n",
    "            name=\"目标点\"\n",
    "        )\n",
    "\n",
    "        # 2. 3D空间图（距离-方位-多普勒）\n",
    "        self.fig_3d = go.FigureWidget(\n",
    "            layout=go.Layout(\n",
