    "            return\n",
    "\n",
    "        # 更新2D极坐标图\n",
    "        filtered_data[\"azimuth_deg\"] = np.rad2deg(filtered_data[\"azimuth_rad\"])\n",
    "        with self.fig_2d.batch_update():\n",
    "            self.fig_2d.data[0].r = filtered_data[\"range_m\"].values\n",
