    "# 雷达串口通信类（适配 profile_2025_09_29T13_36_36_618.cfg 配置）\n",
    "class IWR6843AOP_Serial:\n",
    "    def __init__(self):\n",
    "        self.ser = None  # 串口对象\n",
    "        self.connected = False  # 连接状态标记\n",
    "        self.receiving = False  # 数据接收状态\n",
    "        # 点云数据结构（匹配配置文件中的距离和速度参数）\n",
    "        self.point_cloud = pd.DataFrame(columns=[\n",
    "            \"range_m\",       # 距离（米）\n",
    "            \"azimuth_rad\",   # 方位角（弧度）\n",
    "            \"doppler_ms\",    # 多普勒速度（米/秒）\n",
    "            \"snr_db\"         # 信噪比（分贝）\n",
    "        ])\n",
    "        self.cmd_timeout = 0.5  # 指令超时时间（配置文件指令较多，延长超时）\n",
    "        self.max_retries = 2    # 关键指令重试次数\n",
    "\n",
    "    def scan_pynq_ports(self):\n",
    "        \"\"\"扫描Pynq平台可用串口（仅显示/dev/ttyUSB*或/dev/ttyACM*）\"\"\"\n",
    "        try:\n",
    "            ports = list(serial.tools.list_ports.comports())\n",
    "            valid_ports = [p.device for p in ports if \"ttyUSB\" in p.device or \"ttyACM\" in p.device]\n",
    "            return valid_ports if valid_ports else [\"❌ 未检测到雷达串口（请检查硬件连接）\"]\n",
    "        except Exception as e:\n",
    "            print(f\"⚠️ 串口扫描异常：{str(e)}\")\n",
    "            return [\"❌ 串口扫描失败（权限不足？）\"]\n",
    "\n",
    "    def connect(self, port, baudrate=921600):\n",
    "        \"\"\"连接雷达（配置文件对应波特率921600）\"\"\"\n",
    "        try:\n",
    "            if self.ser and self.ser.is_open:\n",
    "                self.ser.close()\n",
    "            \n",
    "            self.ser = serial.Serial(\n",
    "                port=port,\n",
    "                baudrate=baudrate,\n",
    "                parity=serial.PARITY_NONE,\n",
    "                stopbits=serial.STOPBITS_ONE,\n",
    "                bytesize=serial.EIGHTBITS,\n",
    "                timeout=self.cmd_timeout\n",
    "            )\n",
    "            \n",
    "            # 连接后清空缓存\n",
    "            time.sleep(0.1)\n",
    "            self.ser.read_all()\n",
    "            \n",
    "            if self.ser.is_open:\n",
    "                self.connected = True\n",
    "                return True\n",
    "            return False\n",
    "        except serial.SerialException as e:\n",
    "            print(f\"❌ 串口连接错误：{str(e)}（可能端口被占用）\")\n",
    "            return False\n",
    "        except Exception as e:\n",
    "            print(f\"❌ 连接异常：{str(e)}\")\n",
    "            return False\n",
    "\n",
    "    def disconnect(self):\n",
    "        \"\"\"断开连接（释放资源）\"\"\"\n",
    "        if self.connected and self.ser.is_open:\n",
    "            self.stop_receive()\n",
    "            self.ser.close()\n",
    "            self.connected = False\n",
    "            print(\"✅ 已断开雷达连接\")\n",
    "        elif self.ser:\n",
    "            self.ser.close()\n",
    "\n",
    "    def send_cmd_with_retry(self, cmd, retry_on_fail=True):\n",
    "        \"\"\"带重试的指令发送（适配配置文件的指令序列）\"\"\"\n",
    "        if not self.connected or not self.ser.is_open:\n",
    "            return (False, \"未连接雷达\")\n",
    "        \n",
    "        for retry in range(self.max_retries + 1):\n",
    "            try:\n",
    "                self.ser.read_all()  # 清空缓存\n",
    "                cmd_clean = cmd.strip()\n",
    "                cmd_bytes = (cmd_clean + \"\\r\\n\").encode(\"utf-8\")\n",
    "                self.ser.write(cmd_bytes)\n",
    "                self.ser.flush()\n",
    "                \n",
    "                # 读取响应（适配配置文件中长指令的响应时间）\n",
    "                start_time = time.time()\n",
    "                resp = b\"\"\n",
    "                while (time.time() - start_time) < self.cmd_timeout:\n",
    "                    if self.ser.in_waiting > 0:\n",
    "                        resp += self.ser.read(self.ser.in_waiting)\n",
    "                        time.sleep(0.01)\n",
    "                    else:\n",
    "                        time.sleep(0.005)\n",
    "                \n",
    "                resp_str = resp.decode(\"utf-8\", errors=\"ignore\").strip()\n",
    "                \n",
    "                # 判断指令是否成功（配置文件中的指令成功标志）\n",
    "                if \"Error\" in resp_str or \"error\" in resp_str:\n",
    "                    if retry < self.max_retries and retry_on_fail:\n",
    "                        print(f\"⚠️ 指令 [{cmd_clean}] 第 {retry+1} 次失败，重试中...\")\n",
    "                        time.sleep(0.1)\n",
    "                        continue\n",
    "                    return (False, f\"指令错误：{resp_str}\")\n",
    "                else:\n",
    "                    return (True, resp_str if resp_str else \"无响应（正常）\")\n",
    "            \n",
    "            except serial.SerialException as e:\n",
    "                if retry < self.max_retries and retry_on_fail:\n",
    "                    print(f\"⚠️ 串口异常，指令重试中...\")\n",
    "                    time.sleep(0.1)\n",
    "                    continue\n",
    "                return (False, f\"串口异常：{str(e)}\")\n",
    "            except Exception as e:\n",
    "                return (False, f\"未知错误：{str(e)}\")\n",
    "        \n",
    "        return (False, f\"指令 [{cmd_clean}] 重试 {self.max_retries+1} 次均失败\")\n",
    "\n",
    "    def parse_tlv_data(self, raw_data):\n",
    "        \"\"\"解析TLV格式点云（匹配配置文件中的距离和速度范围）\"\"\"\n",
    "        self.point_cloud = pd.DataFrame(columns=[\"range_m\", \"azimuth_rad\", \"doppler_ms\", \"snr_db\"])\n",
    "        if len(raw_data) < 8:\n",
    "            return\n",
    "        \n",
    "        # 解析TLV头部（小端模式）\n",
    "        try:\n",
    "            tlv_type = struct.unpack(\"<I\", raw_data[0:4])[0]\n",
    "            tlv_len = struct.unpack(\"<I\", raw_data[4:8])[0]\n",
    "        except struct.error:\n",
    "            return\n",
    "        \n",
    "        # 仅处理点云数据TLV（类型为1）\n",
    "        if tlv_type != 1:\n",
    "            return\n",
    "        \n",
    "        # 解析每个点（16字节/点）\n",
    "        point_num = tlv_len // 16\n",
    "        point_bytes = raw_data[8:8+tlv_len]\n",
    "        valid_points = []\n",
    "        \n",
    "        for i in range(point_num):\n",
    "            offset = i * 16\n",
    "            if offset + 16 > len(point_bytes):\n",
    "                break\n",
    "            \n",
    "            try:\n",
    "                range_m = struct.unpack(\"<f\", point_bytes[offset:offset+4])[0]\n",
    "                azimuth_rad = struct.unpack(\"<f\", point_bytes[offset+4:offset+8])[0]\n",
    "                doppler_ms = struct.unpack(\"<f\", point_bytes[offset+8:offset+12])[0]\n",
    "                snr_db = struct.unpack(\"<f\", point_bytes[offset+12:offset+16])[0]\n",
    "                \n",
    "                # 根据配置文件过滤：最大距离9.02m，SNR阈值15dB\n",
    "                if (0 < range_m <= 9.02) and (snr_db >= 15):\n",
    "                    valid_points.append([range_m, azimuth_rad, doppler_ms, snr_db])\n",
    "            except struct.error:\n",
    "                continue\n",
    "        \n",
    "        if valid_points:\n",
    "            self.point_cloud = pd.DataFrame(\n",
    "                valid_points, \n",
    "                columns=[\"range_m\", \"azimuth_rad\", \"doppler_ms\", \"snr_db\"]\n",
    "            )\n",
    "\n",
    "    def _receive_thread(self):\n",
    "        \"\"\"后台数据接收线程\"\"\"\n",
    "        raw_buf = []\n",
    "        error_count = 0\n",
    "        max_errors = 5\n",
    "        self.last_read_time = time.time()\n",
    "        \n",
    "        while self.receiving and self.connected:\n",
    "            try:\n",
    "                if self.ser.in_waiting > 0:\n",
    "                    raw_buf.extend(self.ser.read(self.ser.in_waiting))\n",
    "                    # 配置文件帧周期100ms，调整解析频率\n",
    "                    if len(raw_buf) > 4096 or (len(raw_buf) > 0 and time.time() - self.last_read_time > 0.05):\n",
    "                        self.parse_tlv_data(bytes(raw_buf))\n",
    "                        raw_buf = []\n",
    "                        error_count = 0\n",
    "                        self.last_read_time = time.time()\n",
    "                else:\n",
    "                    time.sleep(0.002)\n",
    "            except Exception as e:\n",
    "                error_count += 1\n",
    "                print(f\"⚠️ 数据接收错误：{str(e)}\")\n",
    "                if error_count >= max_errors:\n",
    "                    print(f\"❌ 连续{max_errors}次错误，停止接收\")\n",
    "                    self.receiving = False\n",
    "                time.sleep(0.1)\n",
    "\n",
    "    def start_receive(self):\n",
    "        \"\"\"启动数据接收\"\"\"\n",
    "        if not self.connected:\n",
    "            print(\"❌ 未连接雷达，无法启动接收\")\n",
    "            return\n",
    "        self.receiving = True\n",
    "        self.thread = Thread(target=self._receive_thread, daemon=True)\n",
    "        self.thread.start()\n",
    "        print(\"✅ 数据接收线程已启动\")\n",
    "\n",
    "    def stop_receive(self):\n",
    "        \"\"\"停止数据接收\"\"\"\n",
    "        self.receiving = False\n",
    "        if hasattr(self, \"thread\") and self.thread.is_alive():\n",
    "            self.thread.join(timeout=1)\n",
    "        print(\"✅ 数据接收线程已停止\")"
   ]
