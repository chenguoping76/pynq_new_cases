    "            self.fig_2d.layout.polar.radialaxis.range = [0, max_range]\n",
    "\n",
    "        # 更新3D空间图\n",
    "        filtered_data[\"x_m\"] = filtered_data[\"range_m\"] * np.cos(filtered_data[\"azimuth_rad\"])\n",
    "        filtered_data[\"y_m\"] = filtered_data[\"range_m\"] * np.sin(filtered_data[\"azimuth_rad\"])\n",
    "        with self.fig_3d.batch_update():\n",
    "            self.fig_3d.data[0].x = filtered_data[\"x_m\"].values\n",
    "            self.fig_3d.data[0].y = filtered_data[\"y_m\"].values\n",
    "            self.fig_3d.data[0].z = filtered_data[\"doppler_ms\"].values\n",
    "            self.fig_3d.data[0].marker.color = filtered_data[\"snr_db\"].values\n",
    "            self.fig_3d.layout.scene.xaxis.range = [-max_range, max_range]\n",
    "            self.fig_3d.layout.scene.yaxis.range = [-max_range, max_range]\n",
    "\n",
    "    def _auto_update_thread(self):\n",
    "        \"\"\"自动更新线程\"\"\"\n",
    "        while self.update_flag and self.serial.connected:\n",
    "            self.update_visualization()\n",
    "            time.sleep(self.update_interval / 1000)\n",
